# ============================================================
# ClawFin v2 â€” Example Environment Variables
# Copy to .env and fill in your values
# Run: npm run gen:keys  to auto-generate wallet + DB keys
# ============================================================

# --- LLM (OpenRouter) ---
# Get from: openrouter.ai (hackathon promo: see README)
OPENROUTER_API_KEY=

# --- XMTP Network ---
XMTP_ENV=dev

# --- Convos Group ID (set after running: npm run setup:group) ---
CONVOS_GROUP_ID=

# --- Agent: ClawBot (Buyer) ---
BUYER_WALLET_KEY=
BUYER_DB_ENCRYPTION_KEY=

# --- Agent: DealDasher (Aggressive Discounter Seller) ---
SELLER_DISCOUNTER_WALLET_KEY=
SELLER_DISCOUNTER_DB_ENCRYPTION_KEY=

# --- Agent: BundleKing (Value Bundler Seller) ---
SELLER_BUNDLER_WALLET_KEY=
SELLER_BUNDLER_DB_ENCRYPTION_KEY=

# --- Agent: PremiumHub (Firm Pricing Seller) ---
SELLER_FIRM_WALLET_KEY=
SELLER_FIRM_DB_ENCRYPTION_KEY=

# --- Agent: FlashDeals (Urgency/Scarcity Seller) ---
SELLER_URGENCY_WALLET_KEY=
SELLER_URGENCY_DB_ENCRYPTION_KEY=

# --- Composio (post-deal Gmail + Sheets) ---
# Get from: platform.composio.dev
COMPOSIO_API_KEY=

# --- User Email (for Gmail deal summary) ---
USER_EMAIL=your@email.com

# --- Google Sheets (for deal log) ---
DEAL_TRACKER_SHEET_ID=

# --- Slack (optional) ---
SLACK_CHANNEL_ID=

# --- Voice (optional) ---
VAPI_API_KEY=
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=
